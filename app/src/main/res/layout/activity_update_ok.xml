<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/map1"
    android:visibility="visible"
    tools:context=".UpdateOk">

    <ImageView
        android:layout_width="29dp"
        android:layout_height="42dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="26dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="40dp"
        app:srcCompat="@drawable/tree4" />

    <ImageView
        android:layout_width="29dp"
        android:layout_height="42dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="26dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="40dp"
        app:srcCompat="@drawable/tree4"
        tools:ignore="RtlCompat" />

    <ImageView
        android:layout_width="29dp"
        android:layout_height="42dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="26dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="40dp"
        app:srcCompat="@drawable/tree4" />

    <ImageView
        android:layout_width="36dp"
        android:layout_height="43dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="108dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="12dp"
        app:srcCompat="@drawable/tree4" />

    <ImageView
        android:layout_width="61dp"
        android:layout_height="38dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/imageView7"
        android:layout_marginEnd="75dp"
        android:layout_marginRight="75dp"
        app:srcCompat="@drawable/tree4"
        tools:ignore="UnknownId" />

    <ImageView
        android:layout_width="43dp"
        android:layout_height="46dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="85dp"
        android:layout_marginRight="85dp"
        android:layout_marginTop="127dp"
        app:srcCompat="@drawable/tree4" />

    <ImageView
        android:layout_width="44dp"
        android:layout_height="48dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="174dp"
        android:layout_marginEnd="104dp"
        android:layout_marginRight="104dp"
        app:srcCompat="@drawable/tree4" />

    <ImageView
        android:layout_width="42dp"
        android:layout_height="41dp"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignEnd="@+id/imageView"
        android:layout_alignRight="@+id/imageView"
        app:srcCompat="@drawable/vt"
        tools:ignore="UnknownId" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="54dp"
        android:layout_height="33dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="64dp"
        android:layout_marginTop="239dp"
        app:srcCompat="@drawable/tree4"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="64dp" />

    <ImageView
        android:id="@+id/staRed"
        android:layout_width="98dp"
        android:layout_height="55dp"
        android:layout_alignLeft="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView"
        android:layout_marginLeft="-15dp"
        android:layout_marginStart="-15dp"
        android:layout_marginTop="182dp"
        android:visibility="visible"
        app:srcCompat="@drawable/water_red" />

    <ImageView
        android:id="@+id/staBlue"
        android:layout_width="201dp"
        android:layout_height="35dp"
        android:layout_above="@+id/imageView"
        android:layout_marginBottom="-236dp"
        android:visibility="invisible"
        app:srcCompat="@drawable/water_blue"
        tools:ignore="DuplicateIds,UnknownId" />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="49dp"
        android:layout_height="44dp"
        android:layout_alignBottom="@+id/imageView"
        android:layout_marginBottom="37dp"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp"
        android:layout_toEndOf="@+id/imageView"
        android:layout_toRightOf="@+id/imageView"
        app:srcCompat="@drawable/tap" />

</RelativeLayout>